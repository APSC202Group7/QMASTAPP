<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qmast.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MenuActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Page1Activity"
            android:label="Data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Page2Activity"
            android:label="Control"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Page3Activity"
            android:label="Diagnostics"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Page4Activity"
            android:label="@string/title_activity_page4"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
